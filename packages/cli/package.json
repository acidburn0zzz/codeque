{
  "name": "@codeque/cli",
  "version": "0.1.0-beta.6",
  "description": "Supercharged multiline code search and replace tool",
  "bin": {
    "codeque": "./bin.js"
  },
  "author": "Jakub Mazurek (@jayu) <jakub.mazurek.dev@gmail.com>",
  "license": "Sustainable Use License",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://codeque.co",
  "main": "dist/cli.js",
  "files": [
    "bin.js",
    "dist/**/*.js",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "release-it": "^15.0.0"
  },
  "dependencies": {
    "colorette": "^2.0.16",
    "commander": "^8.3.0",
    "ora": "^5.0",
    "prettier": "^2.5.1",
    "@codeque/core": "^0.1.0-beta.1"
  },
  "scripts": {
    "cli": "node dist/cli.js",
    "dev": "node dist/dev.js",
    "build": "rm -rf dist && BABEL_ENV=production yarn build:code",
    "build:watch": "rm -rf dist && yarn build:code --watch",
    "build:code": "tsc --project tsconfig.json",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint --ext .js,.ts src",
    "lint:fix": "yarn lint --fix",
    "release": "release-it"
  }
}
